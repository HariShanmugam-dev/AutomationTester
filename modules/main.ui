    <string notr="true">#home{
background-image:url(:/Icons/home_logo.png);
background-position: center center;
    background-repeat: no-repeat;
}


QWidget {
    color: #bfe3fe; /* blue-charcoal 200 - light text on dark */
    font: 10pt &quot;Segoe UI&quot;;
}
#filesList{
 background-color: #05151f; /* blue-charcoal 950 */
   font: 10pt &quot;Segoe UI&quot;;
}

QTableWidgetItem:selected {
    background-color: rgb(191, 227, 254);/* Default background color */
    color: #05151f; /* Default text color */
}
/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
    color: #ffffff;
    background-color: rgba(33, 37, 43, 180);
    border: 1px solid #0b6999; /* blue-charcoal 700 */
    background-image: none;
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 2px solid #19a4de; /* blue-charcoal 500 */
    text-align: left;
    padding-left: 8px;
    margin: 0px;
}
#widget_2 .QPushButton {
    background-color: rgb(191, 227, 254);/* Default background color */
    color: #05151f; /* Default text color */
    border: none;
    border-radius: 12px; /* Set rounded corners */
    padding: 10px 20px; 
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {
    background-color: #05151f; /* blue-charcoal 950 */
    border: 1px solid #0c84bd; /* blue-charcoal 600 */
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Content App */
#contentTopBg {
    background-color: #0d597f; /* blue-charcoal 800 */
}
#contentBottom {
    border-top: 3px solid #0d597f; /* blue-charcoal 800 */
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {
    background-color: #0d597f; /* blue-charcoal 800 */
}
#topLogo {
	 border-image: url(:/Icons/Icon_logo.png) 0 0 0 0 stretch stretch; /* âœ… Stretch and fit */
    background: none;  /* Remove the background-image to avoid conflicts */
    min-width:40px;   
    min-height: 40px;
    max-width: 40px;
    max-height: 40px;
}
#titleLeftApp {
    font: 12pt &quot;Segoe UI&quot;;
}
#titleLeftDescription {
    font: 8pt &quot;Segoe UI&quot;;
    color: #bfe3fe; /* blue-charcoal 200 */
}

/* MENUS */
#topMenu .QPushButton {
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 22px solid transparent;
    background-color: transparent;
    text-align: left;
    padding-left: 44px;
    color: #05151f; /* blue-charcoal 950 - darker text for PushButton */
}
#topMenu .QPushButton:hover {
    background-color: #0b6999; /* blue-charcoal 700 */
}

#topMenu .QPushButton:pressed {
    background-color:  rgb(191, 227, 254);
    color: rgb(255, 255, 255);
}

#pagesContainer .QPushButton {
    color: #05151f; /* blue-charcoal 950 - darker text for PushButton */
    text-align: center;
    background-color: transparent;
}

/* Bottom settings button */
#bottomMenu .QPushButton {
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 20px solid transparent;
    background-color: transparent;
    text-align: left;
    padding-left: 44px;
    color: #05151f; /* blue-charcoal 950 - darker text for PushButton */
}
#bottomMenu .QPushButton:hover {
    background-color: #0b6999; /* blue-charcoal 700 */
}
#bottomMenu .QPushButton:pressed {
    background-color:  rgb(191, 227, 254);
    color: rgb(255, 255, 255);
}

/* Bottom Bar */
#bottomBar {
    background-color: #0d597f; /* blue-charcoal 800 */
}
#bottomBar QLabel {
    font-size: 11px;
    color: #bfe3fe; /* blue-charcoal 200 */
    padding-left: 10px;
    padding-right: 10px;
    padding-bottom: 2px;
}

/* Toggle Button */
#toggleButton {
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 20px solid transparent;
    background-color: #0b6999; /* blue-charcoal 700 */
    text-align: left;
    padding-left: 44px;
    color: #05151f;
}
#toggleButton:hover {
    background-color: #0b6999; /* blue-charcoal 700 */
}
#toggleButton:pressed {
     background-color: rgb(23, 26, 30);
}

/* Top Buttons */
#rightButtons .QPushButton {
    background-color: rgba(255, 255, 255, 0);
    border: none;
    border-radius: 5px;
    color: #05151f; /* blue-charcoal 950 - darker text for PushButton */
}
#rightButtons .QPushButton:hover {
    background-color: #0b6999; /* blue-charcoal 700 */
    border-style: solid;
    border-radius: 4px;
}
#rightButtons .QPushButton:pressed {
    background-color: rgb(23, 26, 30);
    border-style: solid;
    border-radius: 4px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
ScrollBars */
QScrollBar:horizontal {
    border: none;
    background: #0b6999; /* blue-charcoal 700 */
    height: 8px;
    margin: 0px 21px 0 21px;
    border-radius: 0px;
}
QScrollBar::handle:horizontal {
    background: #0c84bd; /* blue-charcoal 600 */
    min-width: 25px;
    border-radius: 4px;
}
QScrollBar::add-line:horizontal {
    border: none;
    background: #0b6999; /* blue-charcoal 700 */
    width: 20px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
    border: none;
    background: #0b6999; /* blue-charcoal 700 */
    width: 20px;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
    background: none;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}
QScrollBar:vertical {
    border: none;
    background: #0b6999; /* blue-charcoal 700 */
    width: 8px;
    margin: 21px 0 21px 0;
    border-radius: 0px;
}
QScrollBar::handle:vertical {
    background: #0c84bd; /* blue-charcoal 600 */
    min-height: 25px;
    border-radius: 4px;
}
QScrollBar::add-line:vertical {
    border: none;
    background: #0b6999; /* blue-charcoal 700 */
    height: 20px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
    border: none;
    background: #0b6999; /* blue-charcoal 700 */
    height: 20px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* Btn Close */
#extraCloseColumnBtn { background-color:rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#extraCloseColumnBtn:hover { background-color:  #0b6999; border-style: solid; border-radius: 4px; }
#extraCloseColumnBtn:pressed { background-color:rgb(23, 26, 30); border-style: solid; border-radius: 4px; }

#extraReloadBtn { background-color:rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#extraReloadBtn:hover { background-color:  #0b6999; border-style: solid; border-radius: 4px; }
#extraReloadBtn:pressed { background-color:rgb(23, 26, 30); border-style: solid; border-radius: 4px; }

#homeWidget .QLabel{
font: 20pt &quot;Segoe UI&quot;;
}
#homeWidget .QPushButton {
    border: none;
    background-color: transparent;
    text-align: left;
    color: #0c84bd; /* blue-charcoal 700 */
}
#homeWidget .QPushButton:hover {
    color: #bfe3fe;
}

#homeWidget .QPushButton:pressed {
    background-color: transparent;
    color: rgb(255, 255, 255);
}

#homeWidget{
border: none;
}</string>
